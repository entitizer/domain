{
  "name": "@textactor/domain",
  "version": "0.1.7",
  "description": "Text Actor code module",
  "main": "./lib/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "pretest": "npm run clean && npm run tsc",
    "test": "ava --no-cache --verbose lib/**/*.test.js",
    "posttest": "npm run clean-test-code",
    "tsc": "tsc",
    "tscw": "tsc -w",
    "preversion": "npm run tsc",
    "postversion": "git push --follow-tags",
    "prepublishOnly": "npm run tsc && npm run clean-test-code",
    "clean": "rimraf ./lib && rimraf ./types",
    "clean-test-code": "rimraf ./lib/**/*.test.js && rimraf ./types/**/*.test.d.ts"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/textactor/domain.git"
  },
  "keywords": [
    "domain",
    "textactor"
  ],
  "author": "Dumitru Cantea",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/textactor/domain/issues"
  },
  "homepage": "https://github.com/textactor/domain#readme",
  "devDependencies": {
    "@types/joi": "^14.0.0",
    "@types/node": "10.12.3",
    "ava": "^0.25.0",
    "debug": "^4.1.0",
    "rimraf": "^2.6.2",
    "typescript": "3.1.6"
  },
  "dependencies": {
    "atonic": "^1.0.4",
    "joi": "^14.0.4",
    "standard-text": "^0.1.1"
  }
}
